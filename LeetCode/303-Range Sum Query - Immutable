/**
 * @param {number[]} nums
 */
var NumArray = function(nums) {
    this.nums = nums;
};

/** 
 * @param {number} i 
 * @param {number} j
 * @return {number}
 */
NumArray.prototype.sumRange = function(i, j) {
    var sum = 0;
    for(var k = i;k<=j;k++) {
        sum += this.nums[k];
    }
    return sum;
};
